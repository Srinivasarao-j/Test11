package com.accenture.lkm;

class Sample {
	Sample(){
		String name = "accenture";
		System.out.println(name);
	}
	Sample(String stdname){
		this();    //first line  & invokes defalt constructors
		System.out.println(stdname);
	}
	void displayInfo() {
		System.out.println("displayinfo method");
	}
	void showInfo() {
		displayInfo();
	}
}
public class Eaxmple1 {
	public static void main(String[] args) {
		Sample s=new Sample("srinu");
		s.showInfo();
	}
}
